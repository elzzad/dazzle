{
  "name": "@elzzad/dazzle-babel-loader",
  "version": "0.1.0-alpha.1",
  "description": "Dazzle (formerly dazzle) is a plugin based configuration utility to create server-rendered universal JavaScript applications with no configuration",
  "main": "./lib/index.cjs",
  "types": "./dts/index.d.ts",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "types": "./dts/index.d.ts",
      "import": "./esm/index.js",
      "require": "./lib/index.cjs"
    },
    "./types": {
      "types": "./dts/types.d.ts",
      "import": "./esm/types.js",
      "require": "./lib/types.cjs"
    },
    "./loader/types": {
      "types": "./dts/loader/types.d.ts",
      "import": "./esm/loader/types.js",
      "require": "./lib/loader/types.cjs"
    },
    "./preset": {
      "types": "./dts/preset.d.ts",
      "import": "./esm/preset.js",
      "require": "./lib/preset.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dts/*"
      ]
    }
  },  "scripts": {
    "rename": "renamer --find js --replace cjs \"lib/**\"",
    "build": "run-s build:clean build:cjs build:esm build:dts rename esm:extensions",
    "build:clean": "git clean -dfX esm lib",
    "build:cjs": "swc src -d lib --config-file ../../.swcrc -C module.type=commonjs",
    "build:esm": "swc src -d esm --config-file ../../.swcrc -C module.type=es6",
    "build:dts": "tsc --project tsconfig.json",
    "esm:extensions": "cross-env  TS_NODE_PROJECT=../../tsconfig.json node -r @swc-node/register ../../.config/esm-transformer.ts",
    "test": "pnpm jest --passWithNoTests --config=../../jest.config.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elzzad/dazzle.git",
    "directory": "packages/dazlle-plugin-webpack5"
  },
  "author": "Ã˜yvind Saltvik",
  "license": "MIT",
  "keywords": [
    "isomorphic",
    "dazzle",
    "reason",
    "ssr",
    "universal",
    "webpack"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@elzzad/dazzle": "0.1.0-alpha.1",
    "@elzzad/dazzle-plugin-webpack5": "0.1.0-alpha.1",
    "webpack": "^5.64.4"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/generator": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.0",
    "@babel/plugin-proposal-numeric-separator": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/traverse": "^7.16.0",
    "@types/babel__core": "^7.1.19",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__template": "^7.4.1",
    "@types/babel__traverse": "^7.18.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-define": "^2.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@elzzad/dazzle": "workspace:*",
    "@elzzad/dazzle-plugin-webpack5": "workspace:*",
    "webpack": "^5.64.4"
  }
}
